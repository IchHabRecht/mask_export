{
    "name": "ichhabrecht/mask-export",
    "description": "Export your mask elements as extension",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3 CMS",
        "Mask",
        "Export",
        "Content Elements",
        "Wizard"
    ],
    "homepage": "https://github.com/IchHabRecht/mask_export",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Nicole Cordes",
            "email": "typo3@cordes.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">= 7.2, < 8.2",
        "typo3/cms-core": "^9.5 || ^10.4 || ^11",
        "typo3/cms-extbase": "^9.5 || ^10.4 || ^11",
        "typo3/cms-extensionmanager": "^9.5 || ^10.4 || ^11",
        "typo3/cms-fluid": "^9.5 || ^10.4 || ^11",
        "mask/mask": "^4.0 || ^5.0 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "typo3/cms-fluid-styled-content": "^9.5 || ^10.4",
        "nimut/testing-framework": "5.x-dev",
        "phpunit/phpunit": "^7.5"
    },
    "replace": {
        "typo3-ter/mask-export": "self.version"
    },
    "autoload": {
        "psr-4": {
            "IchHabRecht\\MaskExport\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IchHabRecht\\MaskExport\\Tests\\": "Tests/"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "typo3/cms": {
            "extension-key": "mask_export",
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "app-dir": ".Build",
            "web-dir": ".Build/public"
        }
    }
}
